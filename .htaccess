RewriteEngine On
RewriteBase /
# For security reasons, Option followsymlinks cannot be overridden.
#Options +FollowSymLinks
Options +SymLinksIfOwnerMatch

RewriteRule (css|jpg|png|gif|js|swf)$ - [L]

# Redirect all traffic from www.travaasa.com <http://www.travaasa.com>  to just travaasa.com <http://travaasa.com> 
#RewriteCond %{HTTP_HOST} !^www\.
#RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteCond %{HTTP_HOST} ^184\.106\.113\.117
RewriteRule (.*) http://www.travaasa.com/$1 [R=301,L]
RewriteRule ^hana/#(.*)$ http://travaasa.com/hana/$1 [R=302,L]

RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

Redirect 301 /austin/accomodations/index.php http://travaasa.com/hana/accomodations/index.php
Redirect 301 /austin/austin-experiences/index.php http://travaasa.com/hana/travaasa-hana-experiences/index.php
Redirect 301 /austin/austin-honeymoon/index.php http://travaasa.com/hana/hana-honeymoon/index.php
Redirect 301 /austin/austinphotos/index.php http://travaasa.com/hana/hanaphotos/index.php
Redirect 301 /austin/austinschedule/index.php http://travaasa.com/hana/hanaschedule/index.php
Redirect 301 /austin/dining/index.php http://travaasa.com/hana/hana-dining-overview/index.php
Redirect 301 /austin/directions/index.php http://travaasa.com/hana/road-to-hana/index.php
Redirect 301 /austin/farm/index.php http://travaasa.com/hana/travaasa-hana-experiences/index.php
Redirect 301 /austin/food-truck/index.php http://travaasa.com/hana/hana-dining-overview/index.php
Redirect 301 /austin/groups-and-meetings/index.php http://travaasa.com/hana/hana-groups-and-meetings/index.php
Redirect 301 /austin/rates-and-packages/index.php http://travaasa.com/hana/hana-rates-and-packages/index.php
Redirect 301 /austin/travaasa-spa-overview/index.php http://travaasa.com/hana/hana-the-spa-at-travaasa-hana/index.php
Redirect 301 /austin/video/index.php http://travaasa.com/hana/video/index.php
Redirect 301 /austin/weddings/index.php http://travaasa.com/hana/weddings/index.php
Redirect 301 /austin/austin-renovation/index.php http://travaasa.com/berkshire-county/index.php

AddType image/svg+xml svg svgz
AddEncoding gzip svgz

# 404 page
ErrorDocument 404 /var/www/html/404.php

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /careers/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /careers/index.php [L]
</IfModule>

