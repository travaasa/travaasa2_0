RewriteEngine On
RewriteBase /
# For security reasons, Option followsymlinks cannot be overridden.
#Options +FollowSymLinks
Options +SymLinksIfOwnerMatch

RewriteRule (css|jpg|png|gif|js|swf)$ - [L]

# Redirect all traffic from www.travaasa.com <http://www.travaasa.com>  to just travaasa.com <http://travaasa.com> 
#RewriteCond %{HTTP_HOST} !^www\.
#RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteCond %{HTTP_HOST} ^184\.106\.113\.117
RewriteRule (.*) http://www.travaasa.com/$1 [R=301,L]
RewriteRule ^hana/#(.*)$ http://travaasa.com/hana/$1 [R=302,L]

RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

Redirect 301 /austin/accomodations http://travaasa.com/hana/accomodations
Redirect 301 /austin/austin-experiences http://travaasa.com/hana/travaasa-hana-experiences
Redirect 301 /austin/austin-honeymoon http://travaasa.com/hana/hana-honeymoon
Redirect 301 /austin/austinphotos http://travaasa.com/hana/hanaphotos
Redirect 301 /austin/austinschedule http://travaasa.com/hana/hanaschedule
Redirect 301 /austin/dining http://travaasa.com/hana/hana-dining-overview
Redirect 301 /austin/directions http://travaasa.com/hana/road-to-hana
Redirect 301 /austin/farm http://travaasa.com/hana/travaasa-hana-experiences
Redirect 301 /austin/food-truck http://travaasa.com/hana/hana-dining-overview
Redirect 301 /austin/groups-and-meetings http://travaasa.com/hana/hana-groups-and-meetings
Redirect 301 /austin/rates-and-packages http://travaasa.com/hana/hana-rates-and-packages
Redirect 301 /austin/travaasa-spa-overview http://travaasa.com/hana/hana-the-spa-at-travaasa-hana
Redirect 301 /austin/video http://travaasa.com/hana/video
Redirect 301 /austin/weddings http://travaasa.com/hana/weddings
Redirect 301 /austin/austin-renovation http://travaasa.com/berkshire-county

AddType image/svg+xml svg svgz
AddEncoding gzip svgz

# 404 page
ErrorDocument 404 /var/www/html/404.php

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /careers/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /careers/index.php [L]
</IfModule>

